class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        if len(s) % 2 == 0:
            options = {")": "(", "}": "{", "]": "["}
            temp = [""]
            for i in s:
                if i not in options:
                    temp.append(i)
                else:
                    if options[i] == temp[-1]:
                        temp.pop()
                    else:
                        temp.append(i)
            if len(temp) == 1:
                return True
            else:
                return False
        else:
            return False
